{
  "manifest_version": 2,
  "name": "Copy-Title-URL-Markdown",
  "description":"閲覧しているページのタイトルとURLをMarkdownのリンク形式でクリップボードへコピーする",
  "version": "1.0",
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
      "name": "Copy title and url"
  },
  "permissions": ["clipboardRead", "tabs"]
}
